{
  "manifest_version": 3,
  "name": "Font Mango",
  "version": "1.0",
  "description": "Scan font details of highlighted text",
  "permissions": ["contextMenus", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
